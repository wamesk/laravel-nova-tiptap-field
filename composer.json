{
    "name": "wamesk/laravel-nova-tiptap-field",
    "description": "Laravel Nova Tiptap editor field.",
    "version": "2.0.1",
    "keywords": [
        "laravel",
        "nova",
        "nova5",
        "tiptap",
        "vue",
        "editor"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "laravel/nova": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Wame\\LaravelNovaTiptapField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wame\\LaravelNovaTiptapField\\Providers\\LaravelNovaTiptapFieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
